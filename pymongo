myclient = pymongo.MongoClient("mongodb://localhost:27017/")
mydb = myclient["mydatabase"]
mycol = mydb["customers"]
                             
for x in mycol.find({"grades" : { "$elemMatch":{"score":{"$gt" :20}}}}):
  print(x)
  
  S)
  import pymongo

myclient = pymongo.MongoClient("mongodb://localhost:27017/")
mydb = myclient["mydatabase"]
mycol = mydb["customers"]
                             
for x in mycol.find({"grades" : { "$elemMatch":{"score":{"$gt" : 80 , "$lt" :100}}}}):
  print(x)
  
